	while(true {
	실행코드
	if (조건) break; // 반복문탈출 => 무한반복 종료
	
	
8. switch
switch (key) {
	case "" :
	}
	
	
9. 배열
	// [] : 인덱스 기준으로 순서대로 저장
	// 1. 1차원 배열 : 자료형[] 배열명 = new int[길이]
	// 2. 2차원 배열 : 자료형[][] 배열명 = new int [행][열]
	